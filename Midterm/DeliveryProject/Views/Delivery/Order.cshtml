@using System.Linq
@using DataLibrary.Models

@model List<Order>
@using System.Collections.Generic
<table class="table">
    <thead>
     <tr>
        <th> Dest1  </th>
        <th> Dest2  </th>
        <th>Type</th>
        <th>Ordered Date</th>
        <th>Customer</th>
     </tr>
    </thead>
    <tbody>
    @foreach(var item in Model)
    {
        <tr>
            <td> @item.DestinationOne </td>
            <td> @item.DestinationTwo </td>
            <td>@item.Type</td>
            <td>@item.OrderedDate</td>
            <td>@item.Customer</td>
        </tr>
    }
    </tbody>
</table> 